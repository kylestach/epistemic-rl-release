<!-- -*- mode: XML -*- -->
<launch>
  <node pkg="usb_cam" type="usb_cam_node" name="camera" respawn="true" respawn_delay="30">
    <param name="~pixel_format" value="yuyv" />
  </node>

  <node pkg="um7" type="um7_driver" name="um7" respawn="true" respawn_delay="30" output="log">
    <param name="~port" type="string" value="/dev/sensors/um7" />
  </node>

  <include file="$(find ublox_gps)/launch/ublox_device.launch">
    <arg name="param_file_dir" value="$(find offroad_bringup)/config" />
    <arg name="param_file_name" value="zed_f9p" />
  </include>
</launch>
